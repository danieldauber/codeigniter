{% extends 'base/backend/base.html.twig' %}

{% block title %}
	{{ page_title }} - {{ title }}
{% endblock %}

{% block navbar %}
	{{ include('backend/navbar.html.twig') }}
{% endblock %}

{% block content %}
<div id="page-wrapper">
	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header">{{ page_title }}</h1>
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<!-- /.row -->
	<div class="row">
		<div class="col-lg-6">
			<div class="panel panel-default">
				<div class="panel-heading">
					{{ page_subtitle }}
				</div>
				<div class="panel-body">
					<div class="row">
						<div class="col-lg-12">

							<h2> Bem vindo {{ session.user.user_object.nome }} </h2>
						</div>

					</div>
					<!-- /.row (nested) -->
				</div>
			</div>
			<!-- /.panel -->
		</div>
	</div>
</div>
<!-- /#page-wrapper -->
{% endblock %}
